<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" prefHeight="275.0" prefWidth="307.0" spacing="4.0" style="-fx-background-color: grey;" stylesheets="@JMetroDarkTheme.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.Admin.AdminWinController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
      <Button mnemonicParsing="false" text="Каталог автомобилей" onAction="#showCarCatalog"/>
      <Button mnemonicParsing="false" text="Каталог запчастей" onAction="#showSparePartCatalog"/>
      <Button mnemonicParsing="false" text="Документация" onAction="#showDocumentation"/>
      <Button mnemonicParsing="false" text="Сотрудники" onAction="#showStaff"/>
      <Button mnemonicParsing="false" text="Назад" onAction="#back">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin></Button>
   </children>
</VBox>
